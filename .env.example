# AWS Lambda and SQS Configuration
SQS_QUEUE_URL="your_sqs_queue_url_here"
DLQ_URL="your_dlq_url_here" # Optional, but good for error handling

# Database Configuration
DB_HOST="localhost" # Or your actual DB host
DB_PORT="5432"
DB_NAME="your_db_name"
DB_USER="your_db_user"
DB_PASSWORD="your_db_password"

# Auth0 Configuration
AUTH0_DOMAIN="your-auth0-domain.auth0.com" # don't add https
AUTH0_AUDIENCE="https://your-auth0-api-audience" # keep https://

# AWS S3 Configuration
S3_IMAGE_OUTPUT_BUCKET="your-s3-output-bucket-name"
FONT_S3_BUCKET="your-s3-font-cache-bucket-name" # For image_processor font caching

# Google Fonts API (Optional, for image_processor)
GOOGLE_API_KEY="your_google_fonts_api_key_here" # Required if FontManager is to fetch from Google Fonts

# Logging Configuration
LOG_LEVEL="DEBUG" # Can be DEBUG, INFO, WARNING, ERROR, CRITICAL

# Placeholder for other potential environment variables if your application grows
# OTHER_SERVICE_API_KEY="your_other_service_api_key"